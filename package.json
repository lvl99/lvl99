{
  "name": "lvl99",
  "version": "1.3.2",
  "description": "Nothing to see here, just another frontend framework",
  "private": false,
  "main": "index.js",
  "scripts": {
    "test": "jest"
  },
  "keywords": [
    "jaff",
    "naff",
    "frontend",
    "framework",
    "browser"
  ],
  "homepage": "https://github.com/lvl99/lvl99",
  "repository": {
    "type": "git",
    "url": "https://github.com/lvl99/lvl99.git"
  },
  "bugs": {
    "url": "https://github.com/lvl99/lvl99/issues",
    "email": "matt@lvl99.com"
  },
  "author": "Matt Scheurich <matt@lvl99.com>",
  "license": "MIT",
  "dependencies": {
    "jquery": "^3.2.1",
    "lodash.merge": "^4.6.0",
    "object-path": "^0.11.4",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "jest": "^20.0.4",
    "babel-jest": "^20.0.3",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^7.3.0",
    "browserify": "^14.3.0",
    "gulp": "^3.9.1",
    "gulp-sourcemaps": "^2.6.0",
    "gulp-streamify": "^1.0.2",
    "gulp-uglify": "^2.1.2",
    "streamify": "^0.2.8",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "ignore": [
      "*.js",
      "*.min.js"
    ]
  },
  "jest": {
    "browser": true,
    "moduleFileExtensions": [
      "js",
      "es6",
      "jsx"
    ],
    "testMatch": [
      "**/__tests__/**/*.(jsx?|es6)",
      "**/?(*.)(spec|test).(jsx?|es6)",
      "<rootDir>/tests/**/*.(jsx?|es6)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/bower_components/"
    ],
    "transform": {
      "^.+\\.{es6,jsx}?$": "babel-jest"
    },
    "verbose": true
  }
}
