{
  "name": "lvl99",
  "version": "1.4.0",
  "description": "Nothing to see here, just another frontend framework",
  "private": false,
  "main": "index.js",
  "scripts": {
    "test": "jest"
  },
  "keywords": [
    "jaff",
    "naff",
    "frontend",
    "framework",
    "browser"
  ],
  "homepage": "https://github.com/lvl99/lvl99",
  "repository": {
    "type": "git",
    "url": "https://github.com/lvl99/lvl99.git"
  },
  "bugs": {
    "url": "https://github.com/lvl99/lvl99/issues",
    "email": "matt@lvl99.com"
  },
  "author": "Matt Scheurich <matt@lvl99.com>",
  "license": "MIT",
  "dependencies": {
    "lodash.merge": "^4.6.0",
    "object-path": "^0.11.4",
    "uuid": "^3.1.0"
  },
  "peerDependencies": {
    "jquery": "^3.2.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^8.0.0",
    "browserify": "^14.5.0",
    "browserify-shim": "^3.8.14",
    "chalk": "^2.3.0",
    "clean-webpack-plugin": "^0.1.17",
    "extend": "^3.0.1",
    "gulp": "^3.9.1",
    "gulp-babel": "^7.0.0",
    "gulp-if": "^2.0.2",
    "gulp-jest": "^3.0.1",
    "gulp-sourcemaps": "^2.6.1",
    "gulp-uglify": "^3.0.0",
    "gulp-util": "^3.0.8",
    "jest": "^21.2.1",
    "jest-cli": "^21.2.1",
    "lazypipe": "^1.0.1",
    "lodash.escaperegexp": "^4.1.2",
    "uglifyjs-webpack-plugin": "^1.0.1",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "webpack": "^3.8.1",
    "webpack-merge": "^4.1.1",
    "webpack-stream": "^4.0.0"
  },
  "browserify": {
    "transform": [
      "browserify-shim",
      "babelify"
    ]
  },
  "browserify-shim": {
    "jquery": "global:jQuery"
  },
  "jest": {
    "moduleFileExtensions": [
      "es6",
      "js",
      "json",
      "jsx"
    ],
    "testMatch": [
      "**/__tests__/**/*.test.js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/bower_components/"
    ],
    "transform": {
      "^.+\\.{es6,jsx}?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "/node_modules/"
    ],
    "verbose": true
  },
  "gulpConfig": {
    "browserify": {
      "bundles": {
        "lvl99": {
          "src": "./index.js",
          "dest": "./dist/lvl99.js"
        }
      }
    },
    "webpack": {
      "compileBundles": {
        "src": "./es6/index.es6",
        "dest": "./dist"
      }
    }
  }
}
