{
  "name": "lvl99",
  "version": "1.4.4",
  "description": "Nothing to see here, just another frontend framework",
  "private": false,
  "main": "index.js",
  "scripts": {
    "test": "jest --no-cache",
    "build": "npm run test && gulp build:development"
  },
  "keywords": [
    "jaff",
    "naff",
    "frontend",
    "framework",
    "browser"
  ],
  "homepage": "https://github.com/lvl99/lvl99",
  "repository": {
    "type": "git",
    "url": "https://github.com/lvl99/lvl99.git"
  },
  "bugs": {
    "url": "https://github.com/lvl99/lvl99/issues",
    "email": "matt@lvl99.com"
  },
  "author": "Matt Scheurich <matt@lvl99.com>",
  "license": "MIT",
  "peerDependencies": {
    "jquery": "^3.2.1"
  },
  "dependencies": {
    "lodash.merge": "^4.6.0",
    "object-path": "^0.11.4",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-es2015-modules-umd": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^8.0.0",
    "chalk": "^2.3.0",
    "clean-webpack-plugin": "^0.1.17",
    "docma": "^1.5.2",
    "extend": "^3.0.1",
    "gulp": "^3.9.1",
    "gulp-babel": "^7.0.0",
    "gulp-concat": "^2.6.1",
    "gulp-if": "^2.0.2",
    "gulp-jest": "^3.0.1",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^2.6.1",
    "gulp-uglify": "^3.0.0",
    "gulp-util": "^3.0.8",
    "jest": "^21.2.1",
    "jest-environment-jsdom": "^21.2.1",
    "jest-localstorage-mock": "^2.1.0",
    "jquery": "^3.2.1",
    "jsdom": "^11.5.1",
    "lazypipe": "^1.0.1",
    "lodash.escaperegexp": "^4.1.2",
    "run-sequence": "^2.2.0",
    "uglifyjs-webpack-plugin": "^1.0.1",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "webpack": "^3.8.1",
    "webpack-merge": "^4.1.1",
    "webpack-stream": "^4.0.0"
  },
  "babel": {
    "env": {
      "development": {
        "presets": [
          [
            "env",
            {
              "targets": {
                "browsers": [
                  "last 2 versions"
                ]
              },
              "modules": "umd",
              "useBuiltIns": true,
              "debug": true
            }
          ]
        ],
        "plugins": [
          "add-module-exports",
          "transform-es2015-modules-umd"
        ]
      },
      "test": {
        "presets": [
          [
            "env",
            {
              "debug": false
            }
          ]
        ]
      }
    }
  },
  "jest": {
    "moduleFileExtensions": [
      "es6",
      "js",
      "json",
      "jsx"
    ],
    "setupFiles": [
      "jest-localstorage-mock"
    ],
    "testMatch": [
      "**/__tests__/**/*.test.{js,es6}"
    ],
    "transform": {
      "^.+\\.es6?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "gulpConfig": {
    "webpack": {
      "compileBundles": {
        "src": "./es6/index.es6",
        "dest": "./dist"
      }
    }
  }
}
